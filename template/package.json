    "moleculer": "^0.8.0"
  },  
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "2.13.1",
    "eslint": "4.2.0",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "lolex": "2.1.1",
    "moleculer": "0.8.2",
    "moleculer-docgen": "0.2.1",
